{
    "name": "workable/translate",
    "description": "A Laravel package for managing translate",
    "keywords": [
        "laravel",
        "translate"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "thuannn",
            "email": "thuannguyen0101.work@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "stichoza/google-translate-php": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "Workable\\Translate\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Workable\\Translate\\TranslateServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php artisan package:discover --ansi"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
