{
    "name": "workable/navigation",
    "description": "A Laravel package for managing navigation",
    "keywords": [
        "laravel",
        "navigation"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "thuannn",
            "email": "thuannguyen0101.work@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4"
    },
    "autoload": {
        "psr-4": {
            "Workable\\Navigation\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
               "Workable\\Navigation\\NavigationServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php artisan package:discover --ansi"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
