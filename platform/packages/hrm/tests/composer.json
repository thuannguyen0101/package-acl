{
    "name": "workable/hrm",
    "description": "A Laravel package for managing HRM",
    "keywords": [
        "laravel",
        "hrm"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "thuannn",
            "email": "thuannguyen0101.work@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "maatwebsite/excel": "^3.1.61"
    },
    "autoload": {
        "psr-4": {
            "Workable\\HRM\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Workable\\HRM\\HRMServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php artisan package:discover --ansi"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
